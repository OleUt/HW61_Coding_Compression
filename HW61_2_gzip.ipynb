    "f_tx = open('./python.txt', 'rb')\n",
    "f_gz = gzip.open('./python_gzip.gz', 'wb', compresslevel=9)\n",
    "f_gz.writelines(f_tx)\n",
    "f_gz.close()\n",
    "f_tx.close()"
